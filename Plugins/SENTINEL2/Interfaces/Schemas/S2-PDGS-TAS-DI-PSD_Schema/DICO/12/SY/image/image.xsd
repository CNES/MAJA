		<xs:restriction base="xs:string">
			<xs:enumeration value="QL"/>
			<xs:enumeration value="FULL_RESOLUTION"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="A_DETECTOR_SIDE">
