						</xs:element>
						<xs:element name="Auxiliary_Data_Info">
							<xs:annotation>
								<xs:documentation>Datastrip Level Auxiliary Data</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="IERS_Bulletin" type="S2:S2_IERS_BULLETIN">
										<xs:annotation>
											<xs:documentation>IERS bulletin. These data are provided for the start acquisition date</xs:documentation>
											<xs:appinfo>
												<xs:DOC>
													<xs:DEFINITION/>
													<xs:CREATED_BY>DATATION</xs:CREATED_BY>
												</xs:DOC>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="GIPP_List" type="S2:S2_GIPP_LIST">
										<xs:annotation>
											<xs:documentation>Reference to the GIPP used</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="metadataLevel" fixed="Standard"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
