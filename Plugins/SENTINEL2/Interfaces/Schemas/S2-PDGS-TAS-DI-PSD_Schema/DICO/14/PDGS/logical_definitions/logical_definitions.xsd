	<xs:simpleType name="A_PDI_ID_ALL">
		<xs:annotation>
			<xs:documentation>PDI identification string (granules, datastrip, SAD)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="S2(A|B)_OPER_\d\d\d\d\d\d\d\d\d\d_\d\d\d\d_\d\d\d\d\d\d\d\dT\d\d\d\d\d\d_\d\d\d\d\d\d\d\dT\d\d\d\d\d\d_\d\d_\d\d\d\d"/>
			<xs:pattern value="S2(A|B)_OPER_\d\d\d\d\d\d\d\d\d\d_\d\d\d\d_\d\d\d\d\d\d\d\dT\d\d\d\d\d\d_\d\d\d\d\d\d_\d\d\d\d\d\d_\d\d\d\d\d_\d\d\d\d"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Latency">
		<xs:annotation>
			<xs:documentation>The archive latency (On-line / Off-line)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="On-line"/>
			<xs:enumeration value="Off-line"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="List_Of_MRFs">
		<xs:annotation>
			<xs:documentation>The list of MRFs used by a component.</xs:documentation>
