													<xs:complexType>
														<xs:sequence>
															<xs:element name="VALUE" type="misc:A_DOUBLE_WITH_M_UNIT_ATTR"/>
															<xs:element name="MEASUREMENT_DATE" type="date_time:A_GPS_DATE_TIME">
																<xs:annotation>
																	<xs:documentation>Measurement date=date provided by GIPP</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="Reference_Geolocation">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="VALUE" type="misc:A_DOUBLE_WITH_M_UNIT_ATTR"/>
