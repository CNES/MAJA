								<xs:documentation>The MRF filename</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ValidityStart" type="date_time:AN_UTC_DATE_TIME">
							<xs:annotation>
								<xs:documentation>The MRF Validity start time</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ValidityStop" type="date_time:AN_UTC_DATE_TIME">
							<xs:annotation>
								<xs:documentation>The MRF Validity stop time</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
