	<xsd:import namespace="http://gs2.esa.int/DICO/1.0/PDGS/component/" schemaLocation="../component/component.xsd"/>
	<xsd:import namespace="http://gs2.esa.int/DICO/1.0/PDGS/center/" schemaLocation="../center/center.xsd"/>
